{
    "builds": [{
        "src": "fitness_tracker_api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python-3.12.7" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "fitness_tracker_api/wsgi.py"
        }
    ]
}